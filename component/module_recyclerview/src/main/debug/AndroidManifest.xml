<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.dryseed.module_recyclerview">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:name="com.easy.moduler.lib.okbus.BaseAppModuleApp"
            android:roundIcon="@drawable/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme" tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <service
                android:name="com.easy.moduler.lib.okbus.NoticeService"
                android:enabled="true"
                android:exported="true">
            <intent-filter>
                <action android:name="com.easy.moduler.lib.okbus.NoticeService"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </service>

        <activity android:name=".TestRecyclerViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".sticky.TestStickyRecyclerViewActivity"
                android:label="StickyRecyclerView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".multiTypeAdapter.TestMultiTypeAdapterActivity"
                android:label="MultiTypeAdapter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".orientationHelper.TestOrientationHelperActivity"
                android:label="OrientationHelper">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".scroll.TestRecyclerViewScrollActivity"
                android:label="RecyclerViewScrollUtils">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".snapHelper.TestSnapHelperActivity"
                android:label="SnapHelper">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".itemTouchHelper.demo1.TestItemTouchHelperActivity"
                android:label="ItemTouchHelper/Demo1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".itemTouchHelper.demo2.TestItemTouchHelper2Activity"
                android:label="ItemTouchHelper/Demo2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".itemDecoration.TestItemDecorationActivity"
                android:label="ItemDecoration">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".itemAnimator.TestItemAnimatorActivity"
                android:label="ItemAnimator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.dryseed.dsdemo.RECYCLERVIEW"/>
            </intent-filter>
        </activity>
    </application>

</manifest>